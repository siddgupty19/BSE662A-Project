        <Param val="i += 1;&amp;#10;msg1 = `Rs ${pot1}`;&amp;#10;msg2 = `Rs ${pot2}`;&amp;#10;thisFrame = all_frames.pop(0);&amp;#10;if ((thisFrame === &quot;I will take&quot;)) {&amp;#10;    thisSplit = `Rs ${offers[i][0]}`;&amp;#10;} else {&amp;#10;    if ((thisFrame === &quot;You will receive&quot;)) {&amp;#10;        thisSplit = `Rs ${offers[i][1]}`;&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
